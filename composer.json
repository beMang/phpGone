{
    "name": "bemang/phpgone_skeleton",
    "description": "exemple de base d'une application avec phpGone",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "version": "0.1.0",
    "require": {
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "twig/twig": "^2.4",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "phpGone\\": "core",
            "phpGoneHelpers\\": "core/Helpers"
        }
    }
}